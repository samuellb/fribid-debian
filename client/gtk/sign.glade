                            <property name="xalign">0</property>
                            <property name="label" translatable="yes">_Prove identity</property>
                            <property name="use_underline">True</property>
                          </widget>
                        </child>
                      </widget>
                      <packing>
                        <property name="position">1</property>
                      </packing>
                    </child>
                  </widget>
                </child>
